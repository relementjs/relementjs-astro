{
	"name": "@relementjs/astro",
	"private": true,
	"version": "0.0.6",
	"description": "relementjs astro integration",
	"keywords": [
		"astrojs",
		"relementjs",
		"astro content"
	],
	"homepage": "https://github.com/relementjs/relementjs-astro#readme",
	"bugs": {
		"url": "https://github.com/relementjs/relementjs-astro/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/relementjs/relementjs-astro.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json"
	],
	"types": "index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"astro": "^4.0.7",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}